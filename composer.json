{
  "name": "feetree/smsc",
  "description": "Package for work with smsc.ru API",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexey Filippov",
      "email": "sobsgm@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "illuminate/support": "^8.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "SMSC\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "SMSC\\ServiceProvider"
      ]
    }
  }
}
